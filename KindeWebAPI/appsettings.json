{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Authority": "https://<your-domain>.kinde.com",
        "ValidAudiences": [
          "<your-audience>"
        ]
      }
    }
  }
}
